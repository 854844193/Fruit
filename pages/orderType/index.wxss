/* pages/orderType/index.wxss */
.order {
  height: 100vh;
  background: #eeeeee;
}

.tabscon {
  padding: 10rpx 20rpx;
}

.Nolist {
  width: 400rpx;
  height: 400rpx;
  margin: 0 auto;
  margin-top: 30vh;
}

.Nolist>image {
  display: block;
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto;
}

.desc {
  text-align: center;
  margin: 20rpx 0;
}

.backBtn {
  width: 200rpx;
  height: 60rpx;
  font-size: 24rpx;
  font-weight: 600;
  border-radius: 30rpx;
  line-height: 60rpx;
  background-color: #ffc20e;
}